{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "file": {
      "class": "logging.FileHandler",
      "filename": "error.txt",
      "mode": "w",
      "level": "ERROR",
      "formatter": "default",
      "encoding": "utf8"
    }
  },
  "loggers": {
    "": {
      "level": "DEBUG",
      "handlers": ["file"]
    }
  }
}
